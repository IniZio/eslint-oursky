{
  "include": ["src/**/*"],
  "compilerOptions": {
    "target": "ES5",
    "module": "ES2015",
    "moduleResolution": "Node",
    "lib": ["ES5", "ES2015.Promise", "ES2015.Collection", "ES2015.Iterable", "DOM"],
    "types": [],
    "jsx": "React",
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  }
}
